{{define "test_results_summary"}}
    <ul>
      <li class="fail" data-status="failed">
        <label>Failed</label>
        <span class="value redtxt">{{.Summary.Failed}}</span>
      </li>
      <li class="pass" data-status="passed">
        <label>Passed</label>
        <span class="value greentxt">{{.Summary.Passed}}</span>
      </li>
      <li class="skip" data-status="skipped">
        <label>Skipped</label>
        <span class="value greytxt">{{.Summary.Skipped}}</span>
      </li>
      <li class="total" data-status="all">
        <label>Total Specs</label>
        <span class="value">{{.Summary.Total}}</span>
      </li>
    </ul>
{{end}}